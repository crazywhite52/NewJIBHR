B,WAA1B,EAAuC,iBAAvC,CAFX;AAGE,MAAA,KAAK,EAAE,KAAKhB,KAAL,CAAWG,SAHpB;AAIE,MAAA,QAAQ,EAAEiB,KAAK,IACb,KAAKX,QAAL,CAAc;AAAEN,QAAAA,SAAS,EAAEiB,KAAK,CAACC,MAAN,CAAa1B;AAA1B,OAAd,CALJ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADF,CA3EF,EAqFE,oBAAC,IAAD;AAAM,MAAA,IAAI,MAAV;AAAW,MAAA,EAAE,EAAE,CAAf;AAAkB,MAAA,EAAE,EAAE,CAAtB;AAAyB,MAAA,EAAE,EAAE,CAA7B;AAAgC,MAAA,EAAE,EAAE,CAApC;AAAuC,MAAA,EAAE,EAAE,CAA3C;AAA8C,MAAA,KAAK,EAAE;AAAEqB,QAAAA,OAAO,EAAE;AAAX,OAArD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACE;AAAI,MAAA,KAAK,EAAE;AAAEC,QAAAA,SAAS,EAAE;AAAb,OAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iKAC2B;AAAG,MAAA,KAAK,EAAE;AAAEC,QAAAA,KAAK,EAAE;AAAT,OAAV;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,WAD3B,CADF,CArFF,EA0FE,oBAAC,IAAD;AAAM,MAAA,IAAI,MAAV;AAAW,MAAA,EAAE,EAAE,CAAf;AAAkB,MAAA,EAAE,EAAE,CAAtB;AAAyB,MAAA,EAAE,EAAE,CAA7B;AAAgC,MAAA,EAAE,EAAE,CAApC;AAAuC,MAAA,EAAE,EAAE,CAA3C;AAA8C,MAAA,KAAK,EAAE;AAAEF,QAAAA,OAAO,EAAE;AAAX,OAArD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACE,oBAAC,QAAD;AAEE,MAAA,IAAI,EAAC,OAFP;AAGE,MAAA,MAAM,EAAC,OAHT;AAIE,MAAA,MAAM,EAAC,QAJT;AAKE,MAAA,IAAI,EAAE,KAAKhB,KAAL,CAAWI,UALnB;AAME,MAAA,QAAQ,EAAGmB,IAAD,IAAU;AAAC,aAAKd,QAAL,CAAc;AAACL,UAAAA,UAAU,EAACmB;AAAZ,SAAd;AAAiC,OANxD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADF,CA1FF,CANF,CADF;AA+GD;;AAlIwC","sourcesContent":["import React, { Component } from \"react\";\r\nimport Grid from \"@material-ui/core/Grid\";\r\nimport {\r\n  Box,\r\n  // Select,\r\n  Grommet,\r\n  TextInput,\r\n  CheckBox,\r\n  RadioButtonGroup,\r\n  Calendar\r\n} from \"grommet\";\r\nimport Select from 'react-select';\r\nvar options = [\r\n  { value: '75', label: 'ออนไลน์' },\r\n  { value: '1000', label: 'สำนักงานใหญ่' },\r\n  { value: '100', label: 'สาขา อยุธยา' }\r\n];\r\nexport default class Job extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      DDbranchname:'สำนักงานใหญ่',\r\n      PTchecked: false,\r\n      OWchecked: '',\r\n      dateSelect:(new Date()).toISOString(),\r\n     \r\n    };\r\n  }\r\n\r\n  getDataContent(varname,value) {\r\n \r\n    this.setState({[varname]:value});\r\n    console.log(value)\r\n  }\r\n\r\n  render() {\r\n    return (\r\n      <div>\r\n        <div>\r\n          {\" \"}\r\n          <h4>ลักษณะงานที่ต้องการ </h4>\r\n          <hr />\r\n        </div>\r\n        <Grid\r\n          container\r\n          spacing={24}\r\n          style={{ marginTop: 10, marginLeft: 10, marginRight: 10 }}\r\n        >\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 5 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              แผนก/สาขา<k style={{ color: \"red\" }}>*</k>\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 5 }}>\r\n            <Select\r\n              placeholder=\"--เลือกแผนก/สาขาที่สมัคร---\"\r\n              size=\"xsmall\"\r\n              style={{width:300}}\r\n               options={options}\r\n               value={this.state.DDbranchname}\r\n               onChange={event => this.getDataContent('DDbranchname',event.target.value)}\r\n            />\r\n            {/* <TextInput\r\n                  // placeholder=\"\"\r\n                  size='small'\r\n                  // value={value}\r\n                  // onChange={event => setValue(event.target.value)}\r\n                /> */}\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 5 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              ตำแหน่งงานที่ต้องการ<k style={{ color: \"red\" }}>*</k>\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={3} xl={3} xs={3} sm={3} md={3} style={{ padding: 5 }}>\r\n            <Select\r\n              placeholder=\"--เลือกแผนก/สาขาที่สมัคร---\"\r\n              // style={{width:'100%'}}\r\n              size=\"xsmall\"\r\n              // dropHeight=\"large\"\r\n              options={[\r\n                \"JIB ONLINE\",\r\n                \"สำนักงานใหญ่\",\r\n                \"แผนกจัดการเทคโนโลยีสารสนเทศ\"\r\n              ]}\r\n              // value={value}\r\n              //  onChange={({ option }) => setValue(option)}\r\n            />\r\n          </Grid>\r\n\r\n          <Grid item lg={3} xl={3} xs={3} sm={3} md={3} style={{ padding: 5 }}>\r\n            <CheckBox\r\n              checked={this.state.PTchecked}\r\n              label=\"PartTime?\"\r\n              onChange={event =>\r\n                this.setState({ PTchecked: event.target.checked })\r\n              }\r\n            />\r\n          </Grid>\r\n\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 5 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              เงินเดือนที่ต้องการ<k style={{ color: \"red\" }}>*</k>\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 5 }}>\r\n            <TextInput\r\n              style={{width:'100%'}}\r\n              size=\"xsmall\"\r\n              // value={value}\r\n              // onChange={event => setValue(event.target.value)}\r\n            />\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 5 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              สถานะภาพการทำงาน(ปัจจุบัน)<k style={{ color: \"red\" }}>*</k>\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={6} xl={6} xs={6} sm={6} md={6} style={{ padding: 5 }}>\r\n            <RadioButtonGroup\r\n              name=\"doc\"\r\n              options={[\"ว่างงาน\", \"ทำงานประจำ\", \"Part Time\", \"กำลังจบการศึกษา\"]}\r\n              value={this.state.OWchecked}\r\n              onChange={event =>\r\n                this.setState({ OWchecked: event.target.value })\r\n              }\r\n            />\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 5 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              วันที่คิดว่าพร้อมเริ่มงาน<k style={{ color: \"red\" }}>*</k>\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 5 }}>\r\n            <Calendar\r\n              \r\n              size=\"small\"\r\n              locale=\"en-US\"\r\n              margin=\"xsmall\"\r\n              date={this.state.dateSelect}\r\n              onSelect={(date) => {this.setState({dateSelect:date})}}\r\n            />\r\n          </Grid>\r\n\r\n        </Grid>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n"]},"metadata":{},"sourceType":"module"}