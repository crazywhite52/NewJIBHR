A,IAAI,MAAV;AAAW,MAAA,EAAE,EAAE,CAAf;AAAkB,MAAA,EAAE,EAAE,CAAtB;AAAyB,MAAA,EAAE,EAAE,CAA7B;AAAgC,MAAA,EAAE,EAAE,CAApC;AAAuC,MAAA,EAAE,EAAE,CAA3C;AAA8C,MAAA,KAAK,EAAE;AAAED,QAAAA,OAAO,EAAE;AAAX,OAArD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACE,oBAAC,SAAD;AACE,MAAA,KAAK,EAAE;AAAEE,QAAAA,KAAK,EAAE;AAAT,OADT;AAEE,MAAA,IAAI,EAAC,QAFP;AAGE,MAAA,QAAQ,EAAEC,KAAK,IACb,KAAKb,QAAL,CAAc;AAAEqB,QAAAA,QAAQ,EAAER,KAAK,CAACX,MAAN,CAAaC;AAAzB,OAAd,EAAgD,MAAM;AACpD,aAAKR,kBAAL;AACD,OAFD,CAJJ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADF,CA1BF,CA/KF,CADF;AAwND;;AAnP4C","sourcesContent":["import React, { Component } from \"react\";\r\nimport InfoIcon from \"@material-ui/icons/Info\";\r\nimport CheckCircleIcon from \"@material-ui/icons/CheckCircle\";\r\nimport Grid from \"@material-ui/core/Grid\";\r\nimport Radio from \"@material-ui/core/Radio\";\r\nimport FormControlLabel from \"@material-ui/core/FormControlLabel\";\r\nimport { withStyles } from \"@material-ui/core/styles\";\r\nimport { green } from \"@material-ui/core/colors\";\r\nimport { TextInput } from \"grommet\";\r\n\r\nexport default class Contact extends Component {\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      title_name: [],\r\n      pass: false\r\n    };\r\n    this.handleOptionChange = this.handleOptionChange.bind(this);\r\n    this.getDataContentFull = this.getDataContentFull.bind(this);\r\n  }\r\n  getDataContentFull(){\r\n    console.log(this.state);\r\n  }\r\n  componentDidMount(){\r\n    console.log(this.state);\r\n  }\r\n  handleOptionChange(e) {\r\n    this.setState({ ...this.state, jib_know: e.target.value },()=>{\r\n      console.log(this.state);\r\n    });\r\n  }\r\n  render() {\r\n    if (this.state.pass === true) {\r\n      var passchk = <CheckCircleIcon style={{ color: \"green\" }} />;\r\n    } else {\r\n      var passchk = <InfoIcon style={{ color: \"orange\" }} />;\r\n    }\r\n    return (\r\n      <div>\r\n        <hr />\r\n        <h4>\r\n          {passchk}\r\n          &nbsp;ข้อมูลบุคคลตที่ติดต่อ\r\n        </h4>\r\n        <Grid\r\n          container\r\n          spacing={24}\r\n          style={{ marginTop: 10, marginLeft: 10, marginRight: 10 }}\r\n        >\r\n          <Grid item lg={1} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              บุคคลที่สามารถติดต่อได้เวลาเร่งด่วน\r\n              \r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <TextInput\r\n              style={{ width: \"100%\" }}\r\n              size=\"xsmall\"\r\n              onChange={event =>\r\n                this.setState({ exi_name: event.target.value }, () => {\r\n                  this.getDataContentFull();\r\n                })\r\n              }\r\n            />\r\n          </Grid>\r\n          <Grid item lg={1} xl={1} xs={1} sm={1} md={1} style={{ padding: 3 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              ความสัมพันธ์\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <TextInput\r\n              style={{ width: \"100%\" }}\r\n              size=\"xsmall\"\r\n              onChange={event =>\r\n                this.setState({ exi_relation: event.target.value }, () => {\r\n                  this.getDataContentFull();\r\n                })\r\n              }\r\n            />\r\n          </Grid>\r\n        </Grid>\r\n        <Grid\r\n          container\r\n          spacing={24}\r\n          style={{ marginTop: 10, marginLeft: 10, marginRight: 10 }}\r\n        >\r\n          <Grid item lg={1} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              เบอร์โทรศัพท์\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <TextInput\r\n              style={{ width: \"100%\" }}\r\n              size=\"xsmall\"\r\n              onChange={event =>\r\n                this.setState({ exi_tel: event.target.value }, () => {\r\n                  this.getDataContentFull();\r\n                })\r\n              }\r\n            />\r\n          </Grid>\r\n          <Grid item lg={1} xl={1} xs={1} sm={1} md={1} style={{ padding: 3 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              เบอร์มือถือ\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <TextInput\r\n              style={{ width: \"100%\" }}\r\n              size=\"xsmall\"\r\n              onChange={event =>\r\n                this.setState({ exi_phone: event.target.value }, () => {\r\n                  this.getDataContentFull();\r\n                })\r\n              }\r\n            />\r\n          </Grid>\r\n        </Grid>\r\n        <Grid\r\n          container\r\n          spacing={24}\r\n          style={{ marginTop: 10, marginLeft: 10, marginRight: 10 }}\r\n        >\r\n          <Grid item lg={1} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              ที่อยู่\r\n              \r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={6} xl={8} xs={8} sm={8} md={6} style={{ padding: 3 }}>\r\n            <TextInput\r\n              style={{ width: \"100%\" }}\r\n              size=\"xsmall\"\r\n              onChange={event =>\r\n                this.setState({ exi_address: event.target.value }, () => {\r\n                  this.getDataContentFull();\r\n                })\r\n              }\r\n            />\r\n          </Grid>\r\n        </Grid>\r\n        <Grid\r\n          container\r\n          spacing={24}\r\n          style={{ marginTop: 10, marginLeft: 10, marginRight: 10 }}\r\n        >\r\n          <Grid item lg={1} xl={1} xs={1} sm={1} md={1} style={{ padding: 20 }}>\r\n            <h6 style={{ textAlign: \"left\" }}>\r\n              รู้จัก JIB ได้ยังไง\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={1} xl={1} xs={1} sm={1} md={1} style={{ padding: 5 }}>\r\n            <FormControlLabel\r\n              checked={this.state.jib_know === \"1\"}\r\n              onChange={this.handleOptionChange}\r\n              value=\"1\"\r\n              control={<Radio color=\"primary\" />}\r\n              label=\"นิตยาสาร\"\r\n              labelPlacement=\"1\"\r\n            />\r\n          </Grid>\r\n          <Grid item lg={1} xl={1} xs={1} sm={1} md={1} style={{ padding: 5 }}>\r\n            <FormControlLabel\r\n              checked={this.state.jib_know === \"2\"}\r\n              onChange={this.handleOptionChange}\r\n              value=\"2\"\r\n              control={<Radio color=\"primary\" />}\r\n              label=\"Internet\"\r\n              labelPlacement=\"2\"\r\n            />\r\n          </Grid>\r\n          <Grid item lg={1} xl={1} xs={1} sm={1} md={1} style={{ padding: 5 }}>\r\n            <FormControlLabel\r\n              checked={this.state.jib_know === \"3\"}\r\n              onChange={this.handleOptionChange}\r\n              value=\"3\"\r\n              control={<Radio color=\"primary\" />}\r\n              label=\"TV\"\r\n              labelPlacement=\"3\"\r\n            />\r\n          </Grid>\r\n          <Grid item lg={1} xl={1} xs={1} sm={1} md={1} style={{ padding: 5 }}>\r\n            <FormControlLabel\r\n              checked={this.state.jib_know === \"4\"}\r\n              onChange={this.handleOptionChange}\r\n              value=\"4\"\r\n              control={<Radio color=\"primary\" />}\r\n              label=\"ป้ายโฆษณา\"\r\n              labelPlacement=\"4\"\r\n            />\r\n          </Grid>\r\n          <Grid item lg={1} xl={1} xs={1} sm={1} md={1} style={{ padding: 20 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n            อื่น ๆ\r\n              \r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={4} xs={4} sm={4} md={4} style={{ padding: 10 }}>\r\n            <TextInput\r\n              style={{ width: \"100%\" }}\r\n              size=\"xsmall\"\r\n              onChange={event =>\r\n                this.setState({ jib_know_other: event.target.value }, () => {\r\n                  this.getDataContentFull();\r\n                })\r\n              }\r\n            />\r\n          </Grid>\r\n        </Grid>\r\n        <hr></hr>\r\n        <Grid\r\n          container\r\n          spacing={24}\r\n          style={{ marginTop: 10, marginLeft: 10, marginRight: 10 }}\r\n        >\r\n          <Grid item lg={1} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              LINE ID:\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <TextInput\r\n              style={{ width: \"100%\" }}\r\n              size=\"xsmall\"\r\n              onChange={event =>\r\n                this.setState({ line: event.target.value }, () => {\r\n                  this.getDataContentFull();\r\n                })\r\n              }\r\n            />\r\n          </Grid>\r\n          <Grid item lg={1} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <h6 style={{ textAlign: \"right\" }}>\r\n              FACEBOOK:\r\n            </h6>\r\n          </Grid>\r\n          <Grid item lg={2} xl={2} xs={2} sm={2} md={2} style={{ padding: 3 }}>\r\n            <TextInput\r\n              style={{ width: \"100%\" }}\r\n              size=\"xsmall\"\r\n              onChange={event =>\r\n                this.setState({ facebook: event.target.value }, () => {\r\n                  this.getDataContentFull();\r\n                })\r\n              }\r\n            />\r\n          </Grid>\r\n          </Grid>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n"]},"metadata":{},"sourceType":"module"}