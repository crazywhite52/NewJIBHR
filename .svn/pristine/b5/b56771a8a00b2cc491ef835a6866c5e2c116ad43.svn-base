ort StepConnector from \"@material-ui/core/StepConnector\";\nimport Button from \"@material-ui/core/Button\";\nimport Typography from \"@material-ui/core/Typography\";\nimport { useAlert } from 'react-alert'\n// import Breadcrumbs from '@material-ui/core/Breadcrumbs';\n\n// PAGE\nimport Job from \"./pages/job\";\n\nfunction getSteps() {\n  return [\n    \"Job Description\",\n    \"Profile\",\n    \"Family\",\n    \"Contact\",\n    \"Education\",\n    \"Training\",\n    \"Career History\",\n    \"Skill\"\n  ];\n}\nconst alert = useAlert()\nclass App extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      activeStep: 0\n    };\n    \n    this.getStepContent = this.getStepContent.bind(this);\n    this.getUpdateContent = this.getUpdateContent.bind(this);\n\n  }\n\n  getStepContent(step) {\n    switch (step) {\n      case 0:\n        return <Job getUpdateContent={this.getUpdateContent} />;\n      case 1:\n        return \"ข้อมูลส่วนตัว\";\n      case 2:\n        return \"ครอบครัว\";\n      case 3:\n        return \"การติดต่อ\";\n      case 4:\n        return \"การศึกษา\";\n      case 5:\n        return \"การฝึกอบรมพิเศษ\";\n      case 6:\n        return \"การทำงาน/ประสบการณ์\";\n      case 7:\n        return \"ความสามารถ\";\n      default:\n        return \"Unknown step\";\n    }\n  }\n\n\n  handleNext = () => {\n    this.setState(state => ({\n      activeStep: state.activeStep + 1\n    }));\n  };\n\n  handleBack = () => {\n    this.setState(state => ({\n      activeStep: state.activeStep - 1\n    }));\n  };\n\n  handleReset = () => {\n    this.setState({\n      activeStep: 0\n    });\n  };\n\n  getUpdateContent=(step,value)=>{\n    console.log(value);\n    \n\n  }\n\n\n  render() {\n    const { classes } = this.props;\n    const steps = getSteps();\n    const connector = (\n      <StepConnector\n        classes={{\n          active: classes.connectorActive,\n          completed: classes.connectorCompleted,\n          disabled: classes.connectorDisabled,\n          line: classes.connectorLine\n        }}\n      />\n    );\n    return (\n      <div>\n        <Grid\n          // container\n          spacing={24}\n          style={{ marginTop: 0, marginLeft: 0, marginRight: 0, flexGrow: 1 }}\n        >\n          {/* <Grid item lg={12} xl={12} xs={12} sm={12} md={12}>\n            <Breadcrumb style={{ fontFamily: \"Patua One\", color: \"#8089a0\" }}>\n              <Breadcrumb.Item>\n                <Grid\n                  container\n                  // spacing={24}\n                  // style={{ marginTop: 10, marginLeft: 10, marginRight: 10 }}\n                >\n                  <k\n                    style={{\n                      fontSize: 30,\n                      color: \"#324b79\",\n                      fontFamily: \"Prompt\"\n                    }}\n                  >\n                    <strong>แบบฟอร์มสมัครงาน</strong>\n                  </k>\n                  <k\n                    style={{\n                      fontSize: 18,\n                      marginTop: 10,\n                      // color: \"#324b79\",\n                      // fontFamily: \"Prompt\"\n                    }}\n                  >\n                    >> jib-Recruitment Online\n                  </k>\n                </Grid>\n              </Breadcrumb.Item>\n            </Breadcrumb>\n          </Grid> */}\n\n          <Grid item lg={12} xl={12} xs={12} sm={12} md={12}>\n            <Grid container style={{backgroundColor:'#e9ecef',paddingTop:10,paddingBottom:10,paddingLeft:15,paddingRight:15}}>\n              <Grid item lg={8} xl={8} xs={8} sm={8} md={8} >\n                <div\n                  style={{ fontFamily: \"Patua One\", color: \"#8089a0\" }}\n                >\n                <k\n                    style={{\n                      fontSize: 30,\n                      color: \"rgb(0, 41, 255)\",\n                      fontFamily: \"Prompt\"\n                    }}\n                  >\n                    <strong>แบบฟอร์มสมัครงาน</strong>\n                  </k>\n                  {'  '}\n                  <k\n                    style={{\n                      fontSize: 18,\n                      marginTop: 10,\n                       color: \"rgb(52, 75, 132)\",\n                      // fontFamily: \"Prompt\"\n                    }}\n                  >\n                    >> jib-Recruitment Online\n                  </k>\n                </div>\n              </Grid>\n              <Grid item lg={4} xl={4} xs={4} sm={4} md={4} style={{textAlign: \"right\"}}>\n              <div\n                  style={{ fontFamily: \"Patua One\", color: \"#8089a0\" , marginTop: 10}}\n                >\n                {/* <div style={{ marginTop: 10 }}> */}\n                  <Button\n                    disabled={this.state.activeStep === 0}\n                    onClick={this.handleBack}\n                    className={classes.button}\n                    style={{fontFamily: \"Patua One\"}}\n                  >\n                    กลับ\n                  </Button>\n                  <Button\n                    variant=\"contained\"\n                    color=\"primary\"\n                    onClick={this.handleNext}\n                    className={classes.button}\n                    color=\"secondary\"\n                    style={{fontFamily: \"Patua One\"}}\n                  >\n                    {this.state.activeStep === steps.length - 1\n                      ? \"บันทึกข้อมูล\"\n                      : \"ถัดไป\"}\n                  </Button>\n                </div>\n                {/* </div> */}\n              </Grid>\n            </Grid>\n          </Grid>\n\n          <Grid item lg={12} xl={12} xs={12} sm={12} md={12}>\n            <Card>\n              {/* <Clock className=\"text-right\" style={{marginTop:5}} type=\"digital\" /> */}\n              <CardContent>\n                \n                <Stepper\n                  activeStep={this.state.activeStep}\n                  connector={connector}\n                >\n                  {steps.map(label => (\n                    <Step key={label}>\n                      <StepLabel>{label}</StepLabel>\n                    </Step>\n                  ))}\n                </Stepper>\n                <div>\n                  {this.state.activeStep === steps.length ? (\n                    <div>\n                      <Typography\n                        className={classes.instructions}\n                        style={{ fontFamily: \"Prompt\", color: \"green\" }}\n                      >\n                        All steps completed - you&apos;re finished\n                      </Typography>\n                    </div>\n                  ) : (\n                    <div>\n                      <Typography\n                        className={classes.instructions}\n                        style={{ fontFamily: \"Prompt\" }}\n                      >\n                        {this.getStepContent(this.state.activeStep)}\n                      </Typography>\n                    </div>\n                  )}\n                </div>\n              </CardContent>\n            </Card>\n          </Grid>\n        </Grid>\n      </div>\n    );\n  }\n}\n\nconst styles = theme => ({\n  root: {\n    width: \"90%\"\n  },\n  button: {\n    marginRight: theme.spacing.unit\n  },\n  instructions: {\n    marginTop: theme.spacing.unit,\n    marginBottom: theme.spacing.unit\n  },\n  connectorActive: {\n    \"& $connectorLine\": {\n      borderColor: theme.palette.secondary.main\n    }\n  },\n  connectorCompleted: {\n    \"& $connectorLine\": {\n      borderColor: theme.palette.primary.main\n    }\n  },\n  connectorDisabled: {\n    \"& $connectorLine\": {\n      borderColor: theme.palette.grey[100]\n    }\n  },\n  connectorLine: {\n    transition: theme.transitions.create(\"border-color\")\n  }\n});\n\nApp.propTypes = {\n  classes: PropTypes.object\n};\n\nexport default withStyles(styles)(App);\n"]},"metadata":{},"sourceType":"module"}